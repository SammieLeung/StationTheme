<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="StationTheme">
    <!--    指定立体按钮的圆角-->
    <attr name="station_3d_button_radius" format="dimension" />
    <!--    指定立体按钮的背景颜色-->
    <attr name="station_3d_button_normal_background_start_color" format="color" />
    <!--    指定立体按钮的背景不可用颜色-->
    <attr name="station_3d_button_disable_background_start_color" format="color" />
    <!--    指定立体按钮的背景起始颜色-->
    <attr name="station_3d_button_focus_background_start_color" format="color" />
    <!--    指定立体按钮的背景结束颜色-->
    <attr name="station_3d_button_focus_background_end_color" format="color" />
    <!--    指定立体按钮的背景起始颜色-->
    <attr name="station_3d_button_press_background_start_color" format="color" />
    <!--    指定立体按钮的背景结束颜色-->
    <attr name="station_3d_button_press_background_end_color" format="color" />
  </declare-styleable>
</resources>